<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
android:id="@+id/kopiframe"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:orientation="vertical"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<RelativeLayout
android:id="@+id/kopitext"
android:layout_width="fill_parent"
android:layout_height="wrap_content">
	<TextView
	android:id="@+id/kopicaption"
	android:layout_width="fill_parent"
	android:layout_height="80px"
	android:layout_marginBottom="5px"
	android:text="KOPI"
	android:textSize="35px"
	android:typeface="serif"
	android:textStyle="bold|italic"
	android:gravity="center"
	android:textColor="#ededed"
	android:shadowColor="#000"
	android:shadowDy="3.0"
	android:shadowDx="2.0"
	android:shadowRadius="1.0"
	>
	</TextView>
</RelativeLayout>
<RelativeLayout
android:id="@+id/milkrow"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:gravity="fill_horizontal">
	<TextView
	android:id="@+id/milkheader"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:text="MILK"
	android:textSize="20sp"
	android:textColor="#ededed"
	android:typeface="sans"
	android:textStyle="bold"
	>
	</TextView>
	<Button
	android:id="@+id/milknone"
	android:layout_width="100dp"
	android:layout_height="wrap_content"
	android:layout_marginLeft="5px"
	android:layout_below="@id/milkheader"
	android:layout_alignLeft="@id/milkrow"
	android:background="@drawable/roundrect"
	android:text="None"
	android:textSize="15sp"
	android:typeface="sans"
	android:textStyle="bold"
	>
	</Button>
	<Button
	android:id="@+id/milkcondensed"
	android:layout_width="100dp"
	android:layout_height="wrap_content"
	android:layout_toRightOf="@id/milknone"
	android:layout_below="@id/milkheader"
	android:layout_marginLeft="5px"
	android:background="@drawable/roundrect"
	android:text="Condensed"
	android:textSize="15sp"
	android:textStyle="bold"
	>
	</Button>
	<Button
	android:id="@+id/milkevaporated"
	android:layout_width="100dp"
	android:layout_height="wrap_content"
	android:layout_toRightOf="@id/milkcondensed"
	android:layout_below="@id/milkheader"
	android:layout_marginLeft="5px"
	android:background="@drawable/roundrect"
	android:text="Evaporated"
	android:textSize="15sp"
	android:typeface="sans"
	android:textStyle="bold"
	>
	</Button>	
</RelativeLayout>
<RelativeLayout
android:id="@+id/strengthrow"
android:layout_width="fill_parent"
android:layout_height="wrap_content">
	<TextView
	android:id="@+id/strengthheader"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"	
	android:layout_marginTop="10px"
	android:text="STRENGTH"
	android:textSize="20sp"
	android:textColor="#ededed"
	android:textStyle="bold"
	>
	</TextView>
	<Button
	android:id="@+id/strengthweak"
	android:layout_width="100px"
	android:layout_height="wrap_content"
	android:layout_below="@id/strengthheader"
	android:layout_marginLeft="5px"
	android:background="@drawable/roundrect"
	android:text="Weak"
	android:textSize="15sp"
	android:typeface="sans"
	android:textStyle="bold"
	>
	</Button>
	<Button
	android:id="@+id/strengthnormal"
	android:layout_width="100px"
	android:layout_height="wrap_content"
	android:layout_toRightOf="@id/strengthweak"
	android:layout_below="@id/strengthheader"
	android:layout_marginLeft="5px"
	android:background="@drawable/roundrect"
	android:text="Normal"
	android:textSize="15sp"
	android:typeface="sans"
	android:textStyle="bold"
	>
	</Button>
	<Button
	android:id="@+id/strengthstrong"
	android:layout_width="100px"
	android:layout_height="wrap_content"
	android:layout_toRightOf="@id/strengthnormal"
	android:layout_below="@id/strengthheader"
	android:layout_marginLeft="5px"
	android:background="@drawable/roundrect"
	android:text="Strong"
	android:textSize="15sp"
	android:typeface="sans"
	android:textStyle="bold"
	>
	</Button>
</RelativeLayout>
<RelativeLayout
android:id="@+id/sweetnessrow"
android:layout_width="fill_parent"
android:layout_height="wrap_content">
	<TextView
	android:id="@+id/sweetnessheader"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="10px"
	android:text="SWEETNESS"
	android:textSize="20sp"
	android:textColor="#ededed"
	android:typeface="sans"
	android:textStyle="bold"
	>
	</TextView>
	<Button
	android:id="@+id/sweetnessnone"
	android:layout_width="80px"
	android:layout_height="wrap_content"
	android:layout_below="@id/sweetnessheader"
	android:background="@drawable/roundrect"
	android:text="None"
	android:textSize="15sp"
	android:typeface="sans"
	android:textStyle="bold"
	>
	</Button>
	<Button
	android:id="@+id/sweetnessless"
	android:layout_width="80px"
	android:layout_height="wrap_content"
	android:layout_toRightOf="@id/sweetnessnone"
	android:layout_below="@id/sweetnessheader"
	android:background="@drawable/roundrect"
	android:text="Less"
	android:textSize="15sp"
	android:typeface="sans"
	android:textStyle="bold"
	>
	</Button>
	<Button
	android:id="@+id/sweetnessnormal"
	android:layout_width="80px"
	android:layout_height="wrap_content"
	android:layout_toRightOf="@id/sweetnessless"
	android:layout_below="@id/sweetnessheader"
	android:background="@drawable/roundrect"
	android:text="Normal"
	android:textSize="15sp"
	android:typeface="sans"
	android:textStyle="bold"
	>
	</Button>
	<Button
	android:id="@+id/sweetnessmore"
	android:layout_width="80px"
	android:layout_height="wrap_content"
	android:layout_toRightOf="@id/sweetnessnormal"
	android:layout_below="@id/sweetnessheader"
	android:background="@drawable/roundrect"
	android:text="More"
	android:textSize="15sp"
	android:typeface="sans"
	android:textStyle="bold"
	>
	</Button>
</RelativeLayout>
<RelativeLayout
android:id="@+id/icerow"
android:layout_width="fill_parent"
android:layout_height="wrap_content">
	<TextView
	android:id="@+id/iceheader"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:text="ICE"
	android:textSize="20sp"
	android:textColor="#ededed"
	android:textStyle="bold"
	>
	</TextView>
	<Button
	android:id="@+id/iceyes"
	android:layout_width="155px"
	android:layout_height="wrap_content"
	android:layout_below="@id/iceheader"
	android:layout_marginLeft="5px"
	android:background="@drawable/roundrect"
	android:text="Yes"
	android:textSize="15sp"
	android:textStyle="bold"
	android:gravity="center"
	>
	</Button>
	<Button
	android:id="@+id/iceno"
	android:layout_width="155px"
	android:layout_height="wrap_content"
	android:layout_toRightOf="@id/iceyes"
	android:layout_below="@id/iceheader"
	android:layout_marginLeft="5px"
	android:background="@drawable/roundrect"
	android:text="No"
	android:textSize="15sp"
	android:textStyle="bold"
	android:gravity="center"
	android:layout_gravity="center_horizontal"
	>
	</Button>
</RelativeLayout>
</LinearLayout>
